<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="org.catFile" table="Category">	
		<id name="catId" column="catId" type="java.lang.String"/>
		<property name="catName" column="catName" type="java.lang.String"/>
		<property name="catDesc" column="catDesc" type="java.lang.String"/>
		
	</class>
</hibernate-mapping>
