<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd" >
<web-app>
	<servlet>
		<servlet-name>Browser</servlet-name>
		<servlet-class>org.BrowserIdentifier</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>HeaderContent</servlet-name>
		<servlet-class>org.HeaderContent</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Auth</servlet-name>
		<servlet-class>org.AuthenticateServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>Browser</servlet-name>
		<url-pattern>/Browser</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>HeaderContent</servlet-name>
		<url-pattern>/HeaderContent</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Auth</servlet-name>
		<url-pattern>/Authenticate</url-pattern>
	</servlet-mapping>
	
</web-app>
