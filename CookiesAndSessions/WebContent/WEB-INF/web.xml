    <display-name>AddCookies</display-name>
    <servlet-name>AddCookies</servlet-name>
    <servlet-class>org.AddCookies</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddCookies</servlet-name>
