  </servlet>
  <servlet-mapping>
    <servlet-name>AddSessions</servlet-name>
    <url-pattern>/AddSessions</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>RetreiveSession</display-name>
